<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <remove src="css/styles-old.css"/>
        <remove src="css/print.css"/>          
    </head>
    <body>
        <referenceContainer name="backend.page" remove="true"/>
        <referenceContainer name="header" remove="true"/>
        <referenceContainer name="page.menu" remove="true"/>
        <referenceContainer name="page-wrapper" remove="true"/>
        <referenceBlock name="logo" remove="true"/>
        <referenceContainer name="root" label="Root">
            <block class="Magestore\Giftvoucher\Block\Adminhtml\Giftvoucher\View" name="giftvoucher.view" template="giftvoucher/print/massprint.phtml"/>
        </referenceContainer>
    </body>
</page>
