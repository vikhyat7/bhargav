<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <!--Navigate to create new stocktaking page -->
    <actionGroup name="GoToNewStockTakingPageActionGroup">
        <annotations>
            <description>Go to the page to create new stocktaking.</description>
        </annotations>
        <amOnPage url="{{AdminStocktakingNewPage.url}}" stepKey="goToStocktakingNew"/>
        <waitForPageLoad stepKey="waitForPageLoad"/>
    </actionGroup>
</actionGroups>
