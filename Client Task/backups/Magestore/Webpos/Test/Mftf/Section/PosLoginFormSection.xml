<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="PosLoginFormSection">
        <element name="username" type="input" selector="#username"/>
        <element name="password" type="input" selector="#password"/>
        <element name="signIn" type="button" selector=".form-group .btn-default" timeout="1"/>
        <element name="continueLogin" type="button" selector=".wrapper-switch-device .block-bottom .btn-default" timeout="1"/>
    </section>
</sections>
