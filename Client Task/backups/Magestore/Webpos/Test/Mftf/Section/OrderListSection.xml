<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="OrderListSection">
        <element name="firstItem" type="text" selector=".wrapper-order-left .items:nth-child(1) li.item:nth-child(1)"/>
        <element name="inputSearch" type="text" selector="//div[contains(@class, 'wrapper-order-left ')]//input[contains(@class, 'input-search')]"/>
        <element name="loader" type="text" selector=".block-order-list .loader-product"/>
        <element name="items" type="button" selector=".block-order-list .items .item"/>
        <element name="orderList" type="button" selector=".block-order-list"/>
    </section>
</sections>
