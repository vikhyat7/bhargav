<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="MenuSection">
        <element name="menuButton" type="text" selector=".fixed-wrapper-header .header-left .toggle-menu"/>
        <element name="backButton" type="text" selector=".fixed-wrapper-header .back-payment"/>

        <element name="menuBlock" type="text" selector=".wrapper-menu"/>
        <element name="menuItem" type="text" selector=".wrapper-menu .item-menu .{{var1}}" parameterized="true"/>
    </section>
</sections>
