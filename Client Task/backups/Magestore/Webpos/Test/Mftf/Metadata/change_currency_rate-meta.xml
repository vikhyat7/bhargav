<operations xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataOperation.xsd">
    <operation name="ChangeCurrencyRate" dataType="change_currency_rate" type="create" auth="adminOauth" url="/V1/pwasampledata/changeCurrencyRate" method="POST">
        <contentType>application/json</contentType>
        <object key="currencyRate" dataType="change_currency_rate">
            <field key="currency_code">string</field>
            <field key="currency_to">string</field>
            <field key="rate">number</field>
        </object>
    </operation>
</operations>
