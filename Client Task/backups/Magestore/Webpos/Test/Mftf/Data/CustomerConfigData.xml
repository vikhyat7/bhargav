    <entity name="require_customer_config" type="customer_config">
        <requiredEntity type="prefix_show">enable_prefix_show</requiredEntity>
        <requiredEntity type="prefix_options">enable_prefix_options</requiredEntity>
        <requiredEntity type="middlename_show">enable_middlename_show</requiredEntity>
