<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="supplier_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Edit Supplier Information</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magestore\DropshipSuccess\Block\Supplier\Form\Edit" name="supplier_edit" template="supplier/account/edit.phtml" cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>
