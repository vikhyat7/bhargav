<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magestore_OrderSuccess::all">

                    <resource id="Magestore_AdjustStock::inventory">
                        <resource id="Magestore_AdjustStock::stockcontrol">
                            <resource id="Magestore_AdjustStock::reservation" title="Reservation Stock" translate="title" sortOrder="999"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
