<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" layout="2columns-left">
	<update handle="customer_account" />
    <head>
          <css src="Mageants_StoreLocator::css/dealer.css" />
          <script src="Mageants_StoreLocator::js/dealer.js"/>
     </head> 
    <body>
      <referenceBlock name="Mageants-dealer-link">
            <arguments>
                <argument name="is_highlighted" xsi:type="boolean">true</argument>
            </arguments>
      </referenceBlock>
      <referenceBlock name="content">
			      <block class="Mageants\StoreLocator\Block\Index" name="storelocator.index.dealer" template="Mageants_StoreLocator::addstore.phtml" />
      </referenceBlock>
    </body>
</page>
